    procedure CreateAndEncodeInputString(ToRecordID: RecordId): Text
    var
        RecRef: RecordRef;
        CompanyInfo: Record "Company Information";
        EncodedInputString: Text;
        Base64InputString: Text;
    begin
        RecRef := ToRecordID.GetRecord();
        CompanyInfo.Get();
        EncodedInputString := EncodeInputString(CompanyInfo.Name, 1, StrLen(CompanyInfo.Name), false);
        EncodedInputString += EncodeInputString(CompanyInfo."VAT Registration No.", 2, StrLen(CompanyInfo."VAT Registration No."), false);
        EncodedInputString += EncodeInputString(GetDocumentPostingDate(ToRecordID), 3, StrLen(GetDocumentPostingDate(ToRecordID)), true);
        EncodedInputString += EncodeInputString(GetDocumentTotalWithVAT(ToRecordID), 4, strlen(GetDocumentTotalWithVAT(ToRecordID)), false);
        EncodedInputString += EncodeInputString(GetDocumentVATTotal(ToRecordID), 5, StrLen(GetDocumentVATTotal(ToRecordID)), false);
        Base64InputString := ConvertToBase64(EncodedInputString, TextEncoding::UTF8);
        Exit(Base64InputString);
    end;

    procedure EncodeInputString(InputString: Text; Tag: Integer; Length: Integer; isDateTime: boolean) theOutput: Text
    begin
        theOutput := '0';
        theOutput += HexValue(Tag);
        if Length < 16 then begin
            theOutput += '0';
            theOutput += HexValue(Length);
        end else
            theOutput += HexValue(Length);
        theOutput += GetBitArray(InputString);
        exit(theOutput);
    end;

    procedure GetTextHexValue(theInput: Text[10]) theOutput: Text[10]
    var
        i: integer;
        intValue: Integer;
        intLeft: Integer;
        intRight: Integer;
    begin
        for i := 1 TO STRLEN(theInput) DO BEGIN
            intValue := theInput[i];
            intLeft := ROUND(intValue / 16, 1, '<');
            intRight := intValue MOD 16;
            theOutput += HexValue(intLeft) + HexValue(intRight);
        END;
    end;

    procedure GetHexBinaryValue(TheInput: Text) theOutput: Text
    var
        i: integer;
        intValue: Integer;
        intLeft: Integer;
        intRight: Integer;
    begin
        // HexToBinray
        for i := 1 TO STRLEN(theInput) DO BEGIN
            theOutput += HexToBinary(TheInput[i]);
        END;
        exit(theOutput);
    end;

    procedure GetBitArray(theInput: Text[2048]) theOutput: Text[2048]
    var
        i: integer;
        intValue: Integer;
    begin
        for i := 1 TO STRLEN(theInput) DO BEGIN
            intValue := theInput[i];
            theOutput += ConvertToUTF8(intValue);
        END;
    end;

