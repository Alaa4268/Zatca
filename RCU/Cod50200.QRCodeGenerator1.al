    procedure ConvertToBase64(TheInput: Text; P_TextEncoding: TextEncoding) TheOutput: Text
    var
        i: integer;
        len: Integer;
        Pos: Integer;
        TextBinaryValue: BigText;
        TextBase64Value: Text;
        Text6BitBinary: array[1000] of Text;
        Padding: Text;
    begin
        i := 1;
        Pos := 1;
        TextBinaryValue.AddText(GetHexBinaryValue(TheInput));
        len := TextBinaryValue.Length;
        Padding := CalculatePadding(len);
        if len <= 6 then begin
            TextBinaryValue.GetSubText(Text6BitBinary[i], Pos, Len - 1);
            TextBase64Value += BinaryToBase64(Text6BitBinary[i]);
        end
        else
            while len > 6 do begin
                TextBinaryValue.GetSubText(Text6BitBinary[i], Pos, 6);
                TextBase64Value += BinaryToBase64(Text6BitBinary[i]);
                Pos += 6;
                i += 1;
                len -= 6;
            END;
        IF Len > 0 THEN begin
            TextBinaryValue.GETSUBTEXT(Text6BitBinary[i], Pos, Len);
            Text6BitBinary[i] := Text6BitBinary[i] + PADSTR('', 6 - strlen(Text6BitBinary[i]), '0');
            TextBase64Value += BinaryToBase64(Text6BitBinary[i]);
        end;

        exit(TextBase64Value + Padding);
    end;

    procedure CalculatePadding(Len: Integer) Padding: Text
    var
        a: Decimal;
        remainder: Decimal;
    begin
        a := Len / 24;
        remainder := (a - Len DIV 24) * 3;
        remainder := ROUND(remainder, 1, '>');
        if remainder = 0 then
            Padding := ''
        else
            if remainder = 1 then
                Padding := '=='
            else
                Padding := '=';
        exit(Padding);
    end;

